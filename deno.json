    "@cliffy/table": "jsr:@cliffy/table@^1.0.0-rc.8",
    "@cliffy/ansi": "jsr:@cliffy/ansi@^1.0.0-rc.8",
    "@linear/sdk": "npm:@linear/sdk",
    "@std/toml": "jsr:@std/toml",
    "@std/fmt": "jsr:@std/fmt",
    "@std/path": "jsr:@std/path",
